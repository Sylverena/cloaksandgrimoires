{
	"schemaVersion": 1,
	"id": "cloaksandgrimoires",
	"version": "${version}",
	"name": "Cloaks and Grimoires",
	"description": "A wizard mod for minecraft with a focus on magic and adventure using the fabric api",
	"authors": [
		"Sylverena",
		"slipcat.xd"
	],
	"contact": {
		"homepage": "https://github.com/Sylverena/cloaksandgrimoires",
		"sources": "https://github.com/Sylverena/cloaksandgrimoires"
	},
	"license": "GPL-3.0",
	"icon": "assets/cloaksandgrimoires/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"com.github.sylverena.cloaksandgrimoires.CloaksAndGrimoires"
		],
		"client": [
			"com.example.ExampleModClient"
		]
	},
	"mixins": [
		"cloaksandgrimoires.mixins.json",
		{
			"config": "cloaksandgrimoires.client.mixins.json",
			"environment": "client"
